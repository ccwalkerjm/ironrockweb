@model IronRock.Objects.Address    
  
        <div class="form-group">
            @Html.LabelFor(model => model.StreetNumberName, new { @class = "control-label col-md-4" })
            <div class="col-md-6">
                @Html.TextBoxFor(model => model.StreetNumberName, new { @class="form-control", required="required"})
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.TownName, new { @class = "control-label col-md-4" })
            <div class="col-md-6">
                @Html.TextBoxFor(model => model.TownName, new { @class="form-control", required="required"})
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.Parish, new { @class = "control-label col-md-4" })
            <div class="col-md-6">
                @Html.TextBoxFor(model => model.Parish, new { @class="form-control", required="required"})
            </div>
        </div>

